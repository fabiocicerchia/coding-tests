{
    "name": "fabiocicerchia/s24-test2",
    "description": "Second Coding Test for S24",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "scripts": {
        "test": "phpdbg -qrr ./vendor/bin/phpunit"
